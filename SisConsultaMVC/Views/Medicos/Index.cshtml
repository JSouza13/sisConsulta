@model IEnumerable<SisConsultaMVC.Models.Medico>

@{
    ViewData["Title"] = "Index";
}

    <div class="row clearfix">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="header">
                    <h2>
                        Excluir paciente
                        @*<small>Cadastro de novo paciente</small>*@
                    </h2>
                    <ul class="header-dropdown m-r--5">
                        <li class="dropdown">
                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <i class="material-icons">more_vert</i>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a href="javascript:void(0);">Action</a></li>
                                <li><a href="javascript:void(0);">Another action</a></li>
                                <li><a href="javascript:void(0);">Something else here</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="body">
                    <div class="row clearfix">

                        <table class="table">
                            <thead>
                                <tr>
                                    <th class="col-md-4">
                                        @Html.DisplayNameFor(model => model.Nome)
                                    </th>
                                    <th class="col-md-4">
                                        @Html.DisplayNameFor(model => model.Especialidade)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Nome)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Especialidade)
                                        </td>
                                        <td class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                            <a type="button" class="btn btn-warning btn-circle waves-effect waves-circle waves-float" data-toggle="tooltip" data-placement="top" title="Editar" asp-action="Edit" asp-route-id="@item.MedicoID">
                                                <i class="material-icons">edit</i>
                                            </a>
                                            <a type="button" class="btn btn-info btn-circle waves-effect waves-circle waves-float" data-toggle="tooltip" data-placement="top" title="Detalhes" asp-action="Details" asp-route-id="@item.MedicoID">
                                                <i class="material-icons">style</i>
                                            </a>
                                            <a type="button" class="btn btn-danger btn-circle waves-effect waves-circle waves-float" data-toggle="tooltip" data-placement="top" title="Excluir" asp-action="Delete" asp-route-id="@item.MedicoID">
                                                <i class="material-icons">delete</i>
                                            </a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                        <hr />
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <a asp-action="Create" class="btn btn-primary btn-lg m-l-15 waves-effect">Novo médico</a>
                            <button type="button" class="btn btn-lg m-l-15 waves-effect" onclick="history.go(-1)">Voltar</button>
                        </div>
